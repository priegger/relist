    "license": "MIT",
    "scripts": {
        "build": "npm run clean-dist && webpack -p",
        "analyze": "ANALYZE=true npm run build",
        "start": "webpack-dev-server --hot",
        "format": "prettier --config .prettierrc.yaml --write '{src,configs,tests}/**/*.{ts,tsx,js,jsx,html}'",
        "format:check": "prettier --config .prettierrc.yaml --list-different '{src,configs,tests}/**/*.{ts,tsx,js,jsx,html}'",
        "lint": "tslint '{src,configs,tests}/**/*.{ts,tsx,js,jsx}'",
        "test": "jest --coverage",
        "test:watch": "jest --watch --coverage",
        "clean-dist": "rm -f -r -d dist"
    },
    "dependencies": {
        "react": "^16.7.0",
        "react-bootstrap": "^1.0.0-beta.5",
        "react-dom": "^16.7.0",
        "react-hot-loader": "^4.6.5",
        "react-redux": "^6.0.0",
        "redux": "^4.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/plugin-proposal-decorators": "^7.2.3",
        "@babel/polyfill": "^7.2.5",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.1.0",
        "@types/enzyme": "^3.1.15",
        "@types/jest": "^23.3.11",
        "@types/node": "^10.12.18",
        "@types/react": "^16.7.18",
        "@types/react-dom": "^16.0.11",
        "@types/react-redux": "=7.0.1",
        "@types/react-test-renderer": "^16.0.3",
        "babel-loader": "^8.0.4",
        "enzyme": "^3.8.0",
        "enzyme-adapter-react-16": "^1.8.0",
        "enzyme-to-json": "^3.3.5",
        "fork-ts-checker-webpack-plugin": "^0.5.2",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^23.6.0",
        "jest-enzyme": "^7.0.1",
        "jest-serializer-html": "^6.0.0",
        "prettier": "1.15.3",
        "react-test-renderer": "^16.7.0",
        "ts-jest": "^23.10.5",
        "tslint": "^5.12.0",
        "tslint-config-prettier": "^1.17.0",
        "typescript": "^3.2.2",
        "webpack": "^4.28.3",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-cli": "^3.2.0",
        "webpack-dev-server": "^3.1.14"
    }
